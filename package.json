    "prepublishOnly": "npm run build",
    "postpublish": "rm -R lib"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "authenticate",
    "authentication",
    "auth",
    "redux"
  ],
  "npmName": "react-authenticated",
  "npmFileMap": [
