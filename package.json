  "author": "Wallace Gleen <wallace.gleen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wgleen/react-authenticated/issues"
  },
  "homepage": "https://github.com/wgleen/react-authenticated#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wgleen/react-authenticated.git"
  },
  "dependencies": {
